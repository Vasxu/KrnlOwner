 local Button = Tab:CreateButton({
    Name = "A-60 and A-120 notifier",
    Callback = function()
        workspace.ChildAdded:Connect(function(Obj)
            if Obj.Name:sub(1, 1) == "A" and Obj.Name ~= "AmbushMoving" then
                game.StarterGui:SetCore("SendNotification", {
                    Title = Obj.Name .. " Spawned ⚠️",
                    Text = "Hide quick",
                    Duration = 5
                })
            end
        end)
        workspace.ChildRemoved:Connect(function(Obj)
            if Obj.Name:sub(1, 1) == "A" and Obj.Name ~= "AmbushMoving" then
                game.StarterGui:SetCore("SendNotification", {
                    Title = Obj.Name .. " DeSpawned ✅",
                    Text = "ur good",
                    Duration = 5
                })
            end
        end)
    end,
 })
 local Button = Tab:CreateButton({
    Name = "POOPDOORS",
    Callback = function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/zoophiliaphobic/POOPDOORS/main/script.lua'))()
    end,
 })

 local Tab = Window:CreateTab("Walkspeed And More", 4483362458)
 local Slider = Tab:CreateSlider({
    Name = "WalksSpeed Changer",
    Range = {0, 20},
    Increment = 10,
    Suffix = "W",
    CurrentValue = 16,
    Flag = "WalkspeedChanger1", -- A flag is the identifier for the configuration file, make sure every element has a different flag if you're using configuration saving to ensure no overlaps
    Callback = function(Value)
    game.workspace.players.localplayer.walkspeed = 20
    game.workspace.players.localplayer.walkspeed = 16
    
    local Paragraph = Tab:CreateParagraph({Title = "Credits", Content = "Zun#5855 -- Main Ui Buttons And More"})
    local Paragraph = Tab:CreateParagraph({Title = "PenguinManiack", Content = "PenguinManiack#2322 -- Made Most Scripts"})
 })

 
