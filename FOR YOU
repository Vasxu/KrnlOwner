local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
    Name = "Gimon",
    LoadingTitle = "The Better Hub.",
    LoadingSubtitle = "by Zun,",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nli, -- Create a custom folder for your hub/game
       FileName = "Gimon Hub"
    },
    Discord = {
       Enabled = true,
       Invite = "KuucUBMFXn", -- The Discord invite code, do not include discord.gg/. E.g. discord.gg/ABCD would be ABCD.
       RememberJoins = true -- Set this to false to make them join the discord every time they load it up
    },
    KeySystem = true, -- Set this to true to use our key system
    KeySettings = {
       Title = "Gimon Key",
       Subtitle = "Key System",
       Note = "Join the discord (discord.gg/Gimon)",
       FileName = "GimonKey",
       SaveKey = true,
       GrabKeyFromSite = false -- If this is true, set Key below to the RAW site you would like Rayfield to get the key from
       Key = "Balls"
    }
 })
 Rayfield:Notify({
    Title = "Welcome coolgqmwsnow!",
    Content = "Sit Back And Relax!",
    Duration = 6.5,
    Image = 4483362458,
    Actions = { -- Notification Buttons
       Ignore = {
          Name = "Okay!",
          Callback = function()
          print("The user tapped Okay!")
       end
    },
 },
 })


 local Doors = Doors:CreateTab("Doors", 4483362458) -- Title, Image

 local DoorsSection = Tab:CreateSection("Main")

 local DoorsButton = Tab:CreateButton({
    Name = "HERE!",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/RegularVynixu/Vynixius/main/Loader.lua"))()
    end,
 })


 local DoorsToggle = Tab:CreateToggle({
    Name = "Walkspeed",
    CurrentValue = false,
    Flag = "Walkspeed", 
    Callback = function(Value)
    game.players.LocalPlayer.Walkspeed = 20
     
    game.Players.localPlayer.walkspeed = 16
    end,
 })

 Rayfield:LoadConfiguration()
